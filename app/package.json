{
  "name": "dockerpress",
  "version": "1.0.1",
  "description": "A Evermade Wordpress Build",
  "main": "gulpfile.js",
  "scripts": {
    "build": "npm run gulp:prod && npm run webpack:prod",
    "dev": "npm-run-all --parallel gulp:dev webpack:dev",
    "gulp:dev": "gulp",
    "gulp:prod": "gulp copy",
    "webpack:dev": "webpack --hide-modules --watch",
    "webpack:prod": "webpack -p"
  },
  "babel": {
    "presets": [
      "react",
      "env"
    ]
  },
  "browserslist": [
    "> 1%"
  ],
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:evermade/dockerpress.git"
  },
  "author": "Mr Evermade",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free-webfonts": "^1.0.4",
    "autoprefixer": "^8.0.0",
    "babel-core": "6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "6.26.0",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.28.10",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.16.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.22.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.10",
    "flickity": "^2.0.10",
    "flightplan": "^0.6.17",
    "font-awesome": "^4.7.0",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-cached": "^1.1.1",
    "gulp-shell": "^0.6.5",
    "gulp-wait": "^0.0.2",
    "gulp-watch": "^5.0.0",
    "headroom.js": "^0.12.0",
    "jquery": "^1.12.0",
    "lodash.throttle": "^4.1.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "remodal": "^1.1.1",
